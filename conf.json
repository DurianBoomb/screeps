{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [ ".", "./task", "./role", "./race", "./policy"],
        "exclude": [ "Gruntfile.js" ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    }
}
